{
  "name": "instance-manager-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --config eslint.config.mjs . --ext .js,.jsx,.ts,.tsx --max-warnings=0 --format stylish",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0 --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "test": "vitest run --reporter=json --no-dom --outputFile=./.report-test.json",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
